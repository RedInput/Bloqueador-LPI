{
  "manifest_version": 2,
  "name" : "AEDE blocker",
  "version" : "1.0",
  "description" : "Bloquea las paginas de aquellos medios asociados a AEDE",
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "icons":
  { 
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background" :
  {
    "scripts": ["check_aede.js"]
  },
  "web_accessible_resources": [
    "blocked_url.html"
  ]
}